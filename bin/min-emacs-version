#!/bin/bash

# Minimum version
min_required_version="24.4"

# Get the version from emacs
# This works also if emacs isn't installed
ret=$(emacs -nw -Q --batch --eval "(if (not (version< emacs-version \"$min_required_version\")) (message \"true\") (message \"false\") )" 2>&1)

if [ "$ret" = "true" ] ; then
	exit 0
else
	exit 1
fi
